
                <h1 class="page-header">Administradores</h1>
                <% if notice %>
                <script>
                $.notify("<%= notice %>", "info");
                </script>
                <% end %>
                <div class="panel panel-default">
                        <div class="panel-heading">
                            Listando Administradores
                            <%= link_to new_backoffice_admin_path, :class => 'btn btn-info pull-right', :style => "margin-top:-6px;" do %>
                            <i class="fa fa-plus"></i>
                            <% end %>
                        </div>
                        <!-- /.panel-heading -->
                        <div class="panel-body">
                            <div class="table-responsive">
                                <table class="table table-striped table-bordered table-hover">
                                    <thead>
                                        <tr>
                                            <th>#</th>
                                            <th>Email</th>
                                            <th>Ações</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                      <% @admins.each do |admin| %>
                                        <tr>
                                            <td><%= admin.id %></td>
                                            <td><%= admin.email %></td>
                                            <td>
                                            <%= link_to  edit_backoffice_admin_path(admin.id), class: "btn btn-warning btn-circle" do %>
                                            <i class="fa fa-check"></i>
                                            <% end %> | <button disabled type="button" class="btn btn-danger btn-circle"><i class="fa fa-times"></i>
                                              </button>
                                        </td>
                                        </tr>
                                        <% end %>
                                    </tbody>
                                </table>
                            </div>
                            <!-- /.table-responsive -->
                        </div>
                        <!-- /.panel-body -->
                    </div>
